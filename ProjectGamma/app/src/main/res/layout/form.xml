<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button"
        android:layout_width="192dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="205dp"
        android:text="Select Course" />

    <TextView
        android:id="@+id/course"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"

        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="29dp"
        android:text="Course Tutored: "
        android:textAllCaps="true"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold" />


    <Button
        android:id="@+id/buttonStartTime"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="337dp"
        android:layout_marginEnd="253dp"
        android:onClick="buttonStartTime"
        android:text="Enter Start Time" />

    <TextView
        android:id="@+id/startTime"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="339dp"
        android:layout_marginEnd="123dp"
        android:text="Start Time: "
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/start"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="338dp"
        android:layout_marginEnd="0dp"
        android:text="0 : 00 "
        android:textAlignment="center"
        android:textSize="20sp" />

    <Button
        android:id="@+id/buttonEndTime"
        android:layout_width="142dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="255dp"
        android:onClick="buttonEndTime"
        android:text="Enter End Time" />

    <TextView
        android:id="@+id/endtime"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="121dp"
        android:text="End Time: "
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/end"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="0dp"
        android:text="0 : 00 "
        android:textAlignment="center"
        android:textSize="20sp" />


    <TextView
        android:id="@+id/venue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/course"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="11dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="90dp"
        android:text="Venue: "
        android:textAllCaps="true"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold" />


    <Button
        android:id="@+id/Confirm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:onClick="send"
        android:text="CONFIRM" />

    <EditText
        android:id="@+id/enterVenue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/venue"
        android:layout_alignBottom="@+id/venue"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:hint="Venue"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/TimeError"
        android:layout_width="372dp"
        android:layout_height="61dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="227dp"
        android:layout_marginEnd="12dp"
        android:textColor="#ff0000"
        android:textStyle="bold"
        android:visibility="visible" />

    <TextView
        android:id="@+id/Sel_Course"
        android:layout_width="192dp"
        android:layout_height="84dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="5dp"
        android:textSize="16sp"
        android:textStyle="bold" />


</RelativeLayout>