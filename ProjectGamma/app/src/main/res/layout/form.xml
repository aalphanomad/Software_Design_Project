<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

<!--
    <Button
        android:id="@+id/button"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="13dp"
        android:text="Select Course" />
        -->


    <TextView
        android:id="@+id/course"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"

        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="29dp"
        android:text="Course Tutored: "
        android:textAllCaps="true"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="UnknownId" />


    <Button
        android:id="@+id/buttonStartTime"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="337dp"
        android:layout_marginEnd="253dp"
        android:onClick="buttonStartTime"
        android:text="Enter Start Time" />

    <TextView
        android:id="@+id/startTime"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="339dp"
        android:layout_marginEnd="123dp"
        android:text="Start Time: "
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/start"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="338dp"
        android:layout_marginEnd="0dp"
        android:text="0 : 00 "
        android:textAlignment="center"
        android:textSize="20sp" />

    <Button
        android:id="@+id/buttonEndTime"
        android:layout_width="142dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="255dp"
        android:onClick="buttonEndTime"
        android:text="Enter End Time" />

    <TextView
        android:id="@+id/endtime"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="121dp"
        android:text="End Time: "
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/end"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="0dp"
        android:text="0 : 00 "
        android:textAlignment="center"
        android:textSize="20sp" />


    <TextView
        android:id="@+id/venue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/course"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="74dp"
        android:text="Venue: "
        android:textAllCaps="true"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Spinner
        android:id="@+id/Activity_Spinner"
        android:layout_width="191dp"
        android:layout_height="37dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="219dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="0dp" />


    <Button
        android:id="@+id/Confirm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:onClick="send"
        android:text="CONFIRM" />

    <EditText
        android:id="@+id/enterVenue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/venue"
        android:layout_alignEnd="@+id/Sel_Course"
        android:layout_alignParentTop="true"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="-91dp"
        android:ems="10"
        android:hint="Venue"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/TimeError"
        android:layout_width="393dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="173dp"
        android:layout_marginEnd="18dp"
        android:textColor="#ff0000"
        android:textStyle="bold"
        android:visibility="visible" />

    <TextView
        android:id="@+id/Sel_Course"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="107dp"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/Activity"

        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="279dp"
        android:text="TYPE OF ACTIVITY:"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Spinner
        android:id="@+id/Course_Spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="75dp" />


</RelativeLayout>